<p id="notice"><%= notice %></p>

<h1>Folders</h1>

<table>
  <thead>
    <tr>
      <th>Soporte estudios</th>
      <th>Referencia laboral</th>
      <th>Fotocopia cedula</th>
      <th>Certificado formacion</th>
      <th>Certificado cuenta bancaria</th>
      <th>Fondo pension</th>
      <th>Cesantias</th>
      <th>Certificado eps</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @folders.each do |folder| %>
      <tr>
        <td><%= folder.soporte_estudios %></td>
        <td><%= folder.referencia_laboral %></td>
        <td><%= folder.fotocopia_cedula %></td>
        <td><%= folder.certificado_formacion %></td>
        <td><%= folder.certificado_cuenta_bancaria %></td>
        <td><%= folder.fondo_pension %></td>
        <td><%= folder.cesantias %></td>
        <td><%= folder.certificado_eps %></td>
        <td><%= folder.user %></td>
        <td><%= link_to 'Show', folder %></td>
        <td><%= link_to 'Edit', edit_folder_path(folder) %></td>
        <td><%= link_to 'Destroy', folder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Folder', new_folder_path %>
